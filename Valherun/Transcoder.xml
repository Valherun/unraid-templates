<?xml version="1.0"?>
<Container version="2">
  <Name>tested</Name>
  <Repository>valherun/Transcoder</Repository>
  <Registry>https://hub.docker.com/r/valherun/transcoder/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support/>
  <Overview>Video Transcoder Adapted from Tested's Transcoder for Don Melton's new Ruby based </Overview>
  <Category>MediaApp:Other Status:Beta</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon/>
  <ExtraParams/>
  <Description>Video Transcoder Adapted from Tested's Transcoder for Don Melton's new Ruby based </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/Archive/transcoder/</HostDir>
      <ContainerDir>/media/transcoder</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/Transcoder</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/unraid</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Config Name="transcoder" Target="/media/transcoder" Default="" Mode="rw" Description="Container Path: /media/transcoder" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/Archive/transcoder/</Config>
  <Config Name="AppData Config Path" Target="/config" Default="" Mode="rw" Description="Container Path: /config" Type="Path" Display="hidden" Required="true" Mask="false"></Config>
  <Config Name="unRAID Share Path" Target="/unraid" Default="/mnt/user" Mode="rw" Description="Container Path: /unraid" Type="Path" Display="hidden" Required="false" Mask="false">/mnt/user</Config>
</Container>
